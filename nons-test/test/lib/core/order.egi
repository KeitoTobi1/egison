assertEqual "compare - case 1"
  (compare 10 10)
  Equal

assertEqual "compare - case 2"
  (compare 11 10)
  Greater

assertEqual "compare - case 3"
  (compare 10 11)
  Less

assertEqual "min"
  (min [20, 5])
  5

assertEqual "min/fn"
  (min/fn compare [10, 20, 5, 20, 30])
  5

assertEqual "max"
  (max [5, 30])
  30

assertEqual "max/fn"
  (max/fn compare [10, 20, 5, 20, 30])
  30

assertEqual "sort"
  (sort [10, 20, 5, 20, 30])
  [5, 10, 20, 20, 30]

assertEqual "sort/fn"
  (sort/fn compare [10, 20, 5, 20, 30])
  [5, 10, 20, 20, 30]
