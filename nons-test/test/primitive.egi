assertEqual("less than predicate",
  [lt?(0.1, 1.0), lt?(1.0, 0.1), lt?(1.0, 1.0)],
  [#t, #f, #f])

assertEqual("less than or equal predicate",
  [lte?(0.1, 1.0), lte?(1.0, 0.1), lte?(1.0, 1.0)],
  [#t, #f, #t])

assertEqual("greater than predicate",
  [gt?(0.1, 1.0), gt?(1.0, 0.1), gt?(1.0, 1.0)],
  [#f, #t, #f])

assertEqual("greater than or equal predicate",
  [gte?(0.1, 1.0), gte?(1.0, 0.1), gte?(1.0, 1.0)],
  [#f, #t, #t])
