  | [] :: [] -> []
  | loop $i (1, $m)
      (($x_i :: $r_i_1) :: (loop $j (2, $n_i)
                              ((#x_i :: $r_i_j) :: ...)
                              (!((#x_i :: _) :: _) & ...)))
      []
  -> map (\i -> Node x_i (listToTree (map (\j -> r_i_j) [1..(n_i)]))) [1..m]

